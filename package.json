{
  "name": "daslab-api-test",
  "version": "0.0.1",
  "description": "Test for DasLab API stack",
  "engines": {
    "node": "12.18.3"
  },
  "devDependencies": {
    "@types/node": "^8.0.29",
    "@types/supertest": "^2.0.10",
    "cross-env": "^7.0.3",
    "jest": "^26.4.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "aws-cloudfront-sign": "^2.2.0",
    "aws-sdk": "^2.749.0",
    "body-parser": "^1.18.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-extended": "^2.8.0",
    "dotenv-parse-variables": "^0.3.1",
    "express": "^4.15.4",
    "express-serve-static-core": "^0.1.1",
    "firebase-admin": "^9.1.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.1.0",
    "i18n": "^0.13.2",
    "json-csv": "^3.0.1",
    "moment": "^2.27.0",
    "mysql": "^2.18.1",
    "pg": "^8.3.3",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^5.0.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "0.2.25",
    "typeorm-encrypted": "^0.5.5",
    "util": "^0.12.3",
    "winston": "^3.3.3",
    "winston-papertrail": "git+https://github.com/kenperkins/winston-papertrail.git#v2",
    "winston-raven-sentry": "^2.0.0",
    "zip-local": "^0.3.4"
  },
  "scripts": {
    "start": "cross-env ENV_FILE=./config/.env.test node dist/src/index.js",
    "build": "tsc --project ./",
    "dev": "cross-env ENV_FILE=./config/.env.test ts-node-dev ./src/index.ts",
    "ts-start": "cross-env ENV_FILE=./config/.env.test ENV=test ts-node src/index.ts",
    "test:unit": "cross-env ENV_FILE=./config/.env.test ENV=test jest",
    "test:coverage": "cross-env ENV_FILE=./config/.env.test ENV=test jest --collect-coverage",
    "db:migration": "typeorm:local migration:generate -n",
    "db:entity": "typeorm:local entity:create -n"
  }
}
